{
    "name": "hristijans/laravel-inertia-table",
    "description": "A flexible and robust table builder for Laravel with Inertia.js and Vue",
    "keywords": [
        "laravel",
        "inertia",
        "vue",
        "table"
    ],
    "homepage": "https://github.com/hristijans/laravel-inertia-table",
    "license": "MIT",
    "authors": [
        {
            "name": "Hristijan Stojanoski",
            "email": "hristijan@example.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.14"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0|^8.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/phpstan": "^1.10",
        "rector/rector": "^0.17"
    },
    "autoload": {
        "psr-4": {
            "Hristijans\\LaravelInertiaTable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hristijans\\LaravelInertiaTable\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hristijans\\LaravelInertiaTable\\LaravelInertiaTableServiceProvider"
            ],
            "aliases": {
                "Table": "Hristijans\\LaravelInertiaTable\\Facades\\Table"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
